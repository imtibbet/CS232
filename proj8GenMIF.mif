-- program memory file
DEPTH = 256;
WIDTH = 16;
ADDRESS_RADIX = DEC;
DATA_RADIX = BIN;
CONTENT
BEGIN
0 : 1111100000000000; -- MOVEI A C Execute RA <= 0
1 : 1001000111000000; -- SUB A B C Execute RA <= RA - ONES
2 : 0110000000000000; -- OPORT S Send register RA to the output port
3 : 0010000000000000; -- BRA L Unconditional branch to address 0
[4..255] : 0011110000000000; -- HALT execute the halt/exit instruction
END